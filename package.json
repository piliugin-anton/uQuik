{
  "name": "uquik",
  "version": "1.0.0",
  "description": "uQuik server",
  "main": "index.js",
  "scripts": {
    "debug": "node --inspect example.js",
    "profile": "node --prof example.js",
    "example": "node example.js",
    "uws": "node benchmark/uWebSockets.js",
    "hyper": "node benchmark/Hyper-Express.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "benchmark": "wrk -t4 -c2500 -d30s http://127.0.0.1:5000",
    "benchmark-uws": "wrk -t4 -c2500 -d30s http://127.0.0.1:5001",
    "benchmark-hyper": "wrk -t4 -c2500 -d30s http://127.0.0.1:5002",
    "benchmark-cannon": "autocannon --renderStatusCodes --debug --warmup [ -c 2500 -d 30 ] -c 2500 -d 30 -p 4 http://127.0.0.1:5000",
    "benchmark-cannon-uws": "autocannon --renderStatusCodes --debug --warmup [ -c 2500 -d 30 ] -c 2500 -d 30 -p 4 http://127.0.0.1:5001",
    "benchmark-cannon-hyper": "autocannon --renderStatusCodes --debug --warmup [ -c 2500 -d 30 ] -c 2500 -d 30 -p 4 http://127.0.0.1:5002"
  },
  "dependencies": {
    "eventemitter3": "^4.0.7",
    "npm": "^8.11.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#binaries"
  },
  "keywords": [
    "uQuik",
    "uWebsockets.js"
  ],
  "author": "Anton Piliugin",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.16.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "nanobench": "^2.1.1"
  }
}