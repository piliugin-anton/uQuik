{
  "name": "uquik",
  "version": "1.0.0",
  "description": "uQuik HTTP(S) framework",
  "main": "index.js",
  "scripts": {
    "debug": "node --inspect example.js",
    "no-sync": "node --trace-sync-io example.js",
    "profile": "node --prof example.js",
    "example": "node example.js",
    "uws": "node benchmark/uWebSockets.js",
    "hyper": "node benchmark/Hyper-Express.js",
    "fastify": "node benchmark/fastify.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "wrk": "wrk -t4 -c2500 -d60s -R68000 http://127.0.0.1:5000",
    "wrk-post": "wrk -s post.lua -t4 -c2500 -d60s -R31000 http://127.0.0.1:5000",
    "wrk-uws": "wrk -t4 -c2500 -d60s -R108000 http://127.0.0.1:5001",
    "wrk-hyper": "wrk -t4 -c2500 -d60s http://127.0.0.1:5002",
    "wrk-hyper-post": "wrk -s post.lua -t4 -c2500 -d60s http://127.0.0.1:5002",
    "wrk-fastify": "wrk -t4 -c2500 -d60s http://127.0.0.1:5003",
    "autocannon": "autocannon --renderStatusCodes --debug --warmup [ -c 2500 -d 30 ] -c 2500 -d 30 -p 4 http://127.0.0.1:5000",
    "autocannon-uws": "autocannon --renderStatusCodes --debug --warmup [ -c 2500 -d 30 ] -c 2500 -d 30 -p 4 http://127.0.0.1:5001",
    "autocannon-hyper": "autocannon --renderStatusCodes --debug --warmup [ -c 2500 -d 30 ] -c 2500 -d 30 -p 4 http://127.0.0.1:5002",
    "autocannon-fastify": "autocannon --renderStatusCodes --debug --warmup [ -c 2500 -d 30 ] -c 2500 -d 30 -p 4 http://127.0.0.1:5003",
    "clinic-doctor": "clinic doctor -- node example.js",
    "clinic-bubbleprof": "clinic bubbleprof -- node example.js",
    "clinic-flame": "clinic flame -- node example.js",
    "clinic-doctor-uws": "clinic doctor -- node benchmark/uWebSockets.js"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "fast-uri": "^2.0.0",
    "readable-stream": "^3.6.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.10.0"
  },
  "keywords": [
    "uQuik",
    "Hyper-Express",
    "uWebsockets.js"
  ],
  "author": "Anton Piliugin",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.16.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "nanobench": "^2.1.1"
  }
}
